const number = [1, 2, 80, 33, 65, 3, 99]

number.sort((a, b) => {
    if (a > b) return 1;
    if (a < b) return -1;
    else return 0;
})

console.log(number)

number.sort((a, b) => {
    if (a < b) return 1;
    if (a > b) return -1;
    else return 0;
})
console.log(number)

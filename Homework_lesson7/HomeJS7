function reverseArr1(Arr) { 
    return Arr.reverse();
  }
  const currentArr = [45, 2, 1, 85, 59];
  const reversedArr = reverseArr1(currentArr);
  
  console.log(reversedArr1);

//  ----------------------------------------------------------------------- 

function maxNumber(numbers) {
    return Math.max(...numbers);
  }
  
  const numbers = [45, 1, 8574, 2, 33, 999];
  const maxValue = maxNumber(numbers);
  
  console.log(maxValue);

//   ---------------------------------------------------
function getFibonachi(number) {
    return number <= 1 ? number : getFibonachi(number - 1) + getFibonachi(number - 2);
};

console.log(getFibonachi(54))

